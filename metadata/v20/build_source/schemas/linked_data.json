{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "general.json#metadata/v20/schemas/linked_data.json",
    "type": "object",
    "properties": {
        "@id": {
            "description": "A Uniform Resource Identifier (URI) that links the resource via the OpenEnergyDatabus (DBpedia Databus).",
            "example": "https://databus.openenergyplatform.org/oeplatform/supply/wri_global_power_plant_database/2022-11-07",
            "type": [
                "string",
                "null"
            ],
            "badge": "Platinum",
            "title": "@Id",
            "readonly": true
        },
        "@context": {
            "description": "Explanation of metadata keys in ontology terms.",
            "example": "https://github.com/OpenEnergyPlatform/oemetadata/blob/production/metadata/latest/context.json",
            "type": [
                "string",
                "null"
            ],
            "badge": "Platinum",
            "title": "@context"
        }
    }
}